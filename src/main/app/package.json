{
  "name": "nivio",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-svg-pan-zoom": "^3.3.1",
    "@types/react-transition-group": "^4.4.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "html-react-parser": "^0.10.5",
    "node-sass": "^4.14.1",
    "raw.macro": "^0.3.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-svg-pan-zoom": "^3.7.1",
    "react-svg-pan-zoom-loader": "^1.4.1",
    "react-transition-group": "^4.4.1",
    "svg-path-properties": "^1.0.4",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --config .prettierrc --c ./src/*.{js,jsx,ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:check",
      "pre-push": "yarn test:ci && yarn lint && yarn format:check"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/src/index.tsx",
      "!**/src/serviceWorker.ts",
      "!**/vendor/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
